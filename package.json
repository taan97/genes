{
  "name": "p2-3-2-front-interface",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "dev:https": "SELF_SIGNED_CERTIFICATE=https vite --host",
    "prebuild": "sh ./simplelocalize-download.sh && npm run format",
    "prebuild:test": "sh ./simplelocalize-download.sh && npm run format",
    "postbuild": "zip -r dist.zip dist",
    "postbuild:test": "zip -r dist.zip dist",
    "build": "vite build",
    "build:test": "vite build --mode development",
    "preview": "vite preview --port 4173 --host",
    "preview:https": "SELF_SIGNED_CERTIFICATE=https vite preview --port 4173 --host",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write --list-different .",
    "prepare": "husky install && sh ./simplelocalize-install.sh"
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^2.0.0",
    "@innovatrics/dot-document-auto-capture": "^3.5.1",
    "@innovatrics/dot-face-auto-capture": "^3.5.1",
    "@vant/touch-emulator": "^1.4.0",
    "@vant/use": "^1.4.1",
    "@videojs-player/vue": "^1.0.0",
    "axios": "^0.27.2",
    "chart.js": "^3.9.1",
    "copy-to-clipboard": "^3.3.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.6",
    "html5-qrcode": "^2.2.1",
    "pinia": "^2.0.17",
    "qrcode": "^1.5.1",
    "signature_pad": "^4.1.4",
    "uuid": "^8.3.2",
    "vant": "^3.5.4",
    "video.js": "^7.20.3",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-pdf-embed": "^1.1.4",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@vitejs/plugin-basic-ssl": "^0.1.2",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.21.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.4",
    "vite-svg-loader": "^3.4.0"
  },
  "lint-staged": {
    "*.{html,css}": "prettier --write",
    "*.{vue,js,ts}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  }
}
